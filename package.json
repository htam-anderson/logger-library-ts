{
  "name": "logger-library-ts",
  "version": "0.0.1",
  "description": "The library to write log",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "tsc": "tsc",
    "prettier:cli": "prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "eslint": "eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "clean:build": "rm -rf ./lib",
    "clean": "npm run clean:build && rm -rf *.tsbuildinfo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/htam-anderson/logger-library-ts.git"
  },
  "keywords": [
    "typescript",
    "library",
    "log",
    "logger"
  ],
  "author": "htam.anderson@gmail.com",
  "license": "",
  "bugs": {
    "url": "https://github.com/ORESoftware/typescript-library-skeleton.git/issues"
  },
  "homepage": "https://github.com/ORESoftware/typescript-library-skeleton.git#readme",
  "dependencies": {
    "moment": "^2.29.2"
  },
  "devDependencies": {
    "@types/node": "^10.17.17",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "eslint-plugin-mocha": "^6.3.0",
    "prettier": "^2.0.4",
    "typescript": "~3.8.3"
  }
}
